{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "object-curly-spacing": "off",
    "max-len": "off"
  }
}
